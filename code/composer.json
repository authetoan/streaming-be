{
    "name": "travis/streaming-backend",
    "description": "Streaming backend",
    "autoload": {
        "psr-4": {
            "Travis\\StreamingBackend\\": "app/",
            "Database\\Migrations\\": "database/migrations/",
            "Test\\" : "tests/",
            "Feature\\" : "features/"
        }
    },
    "authors": [
        {
            "name": "Travis \"Toan\" Au The"
        }
    ],
    "require": {
        "php" : "^8.2",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4",
        "php-di/slim-bridge": "^3.4",
        "vlucas/phpdotenv": "^5.5",
        "symfony/var-dumper": "^6.3",
        "hassankhan/config": "^3.1",
        "doctrine/migrations": "^3.6",
        "league/oauth2-server": "^8.5",
        "nesbot/carbon": "^2.68",
        "ramsey/uuid": "^4.7"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.99",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "~8.0",
        "vimeo/psalm": "^5.13",
        "mockery/mockery": "^1.6",
        "behat/behat": "^3.13"


    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
